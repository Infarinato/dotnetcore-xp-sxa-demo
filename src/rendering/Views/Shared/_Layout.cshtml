@model aspnet_core_demodotcomsite.Models.Layout?

<!DOCTYPE html>
<html lang="en">
<head>
    <title>@Model?.DisplayName</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <link rel="stylesheet" href="/styles/bootstrap-5.1.3.min.css">
    <link rel="stylesheet" href="/styles/skatepark.min.css" />
    <base href="~/" />
    <component type="typeof(Microsoft.AspNetCore.Components.Web.HeadOutlet)" render-mode="ServerPrerendered" />
</head>

<body>
<div id="__next"></div>
    @RenderBody()
    @* ReSharper disable once Html.PathError *@
    <script src="_framework/blazor.server.js"></script>
</body>
</html>