{
    "namespace": "Items",
    "items" : {
        "includes": [
            {
                "name": "templates",
                "path": "/sitecore/templates/Project/HCC Demo site collection",
                "allowedPushOperations": "createUpdateAndDelete",
                "scope": "itemAndDescendants"
            },
            {
                "name": "layouts",
                "path": "/sitecore/layout/Layouts/Project/HCC Demo site collection"
            },
            {
                "name": "content-root",
                "path": "/sitecore/content/HCC Demo site collection",
                "allowedPushOperations": "createUpdateAndDelete",
                "scope": "itemAndDescendants"
            },
            {
                "name": "media",
                "path": "/sitecore/media library/Project/HCC Demo site collection",
                "allowedPushOperations": "createUpdateAndDelete",
                "scope": "itemAndDescendants"
            },
            {
                "name": "renderings",
                "path": "/sitecore/layout/Renderings/Project/HCC Demo site collection",
                "allowedPushOperations": "createUpdateAndDelete",
                "scope": "itemAndDescendants"
            },
            {
                "name": "placeholders",
                "path": "/sitecore/layout/Placeholder Settings/Project/HCC Demo site collection"
            },
            {
                "name": "api-key",
                "path": "/sitecore/system/Settings/Services/API Keys/dotnetcore-xp-sxa-demo",
                "scope": "singleItem"
            },
            {
                "name": "rendering-host",
                "path": "/sitecore/system/Settings/Services/Rendering Hosts/Default",
                "scope": "singleItem"
            },
            {
                "name": "project-folder",
                "path": "/sitecore/templates/Foundation/Experience Accelerator/Multisite/Project Folder",
                "scope": "singleItem"
            }
        ]
    }
}